{
  "name": "firebase-react",
  "version": "1.1.2",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@hookform/resolvers": "^0.1.1",
    "axios": "^0.20.0",
    "bootstrap": "^5.0.0-alpha2",
    "bootstrap-icons": "^1.0.0",
    "customize-cra": "^1.0.0",
    "firebase": "^7.24.0",
    "form-serialize": "^0.7.2",
    "history": "^5.0.0",
    "i18next": "^19.7.0",
    "mobx": "^6.0.1",
    "mobx-react": "^6.3.1",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "query-string": "^6.13.1",
    "react": "^17.0.0-rc.3",
    "react-app-rewired": "^2.1.6",
    "react-avatar-editor": "^12.0.0-beta.0",
    "react-datepicker": "^2.16.0",
    "react-dom": "^17.0.0-rc.2",
    "react-hook-form": "^6.8.4",
    "react-i18next": "^11.7.0",
    "react-number-format": "^4.3.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-router-hash-link": "^2.1.0",
    "react-scripts": "^4.0.0-next.98",
    "react-uuid": "^1.0.2",
    "use-deep-compare-effect": "^1.4.0",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:functions": "export GOOGLE_APPLICATION_CREDENTIALS=../firebase-config.json && firebase emulators:start  --only functions",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "patch": "yarn version --patch && git push",
    "minor": "yarn version --minor && git push",
    "major": "yarn version --major && git push",
    "merge": "git push origin dev:master",
    "deploy": "yarn build && firebase deploy --only=hosting",
    "deploy:functions": "yarn build && firebase deploy --only=functions",
    "deploy:all": "yarn build && firebase deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  }
}
